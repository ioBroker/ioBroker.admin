{
    "tasksRunnerOptions": {
        "default": {
            "runner": "nx/tasks-runners/default",
            "options": {
                "cacheableOperations": [],
                "parallel": 5
            }
        }
    },
    "targetDefaults": {
        "build": {
            "dependsOn": ["^build", "prebuild"]
        },
        "test": {
            "dependsOn": ["build"]
        }
    },
    "projects": {
        "admin": {
            "implicitDependencies": ["jsonConfig", "adapter-react-v5", "dm-gui-components"]
        },
        "jsonConfig": { "implicitDependencies": ["adapter-react-v5"] },
        "dm-gui-components": { "implicitDependencies": ["jsonConfig", "adapter-react-v5"] }
    },
    "$schema": "./node_modules/nx/schemas/nx-schema.json",
    "namedInputs": {
        "default": ["{projectRoot}/**/*", "sharedGlobals"],
        "sharedGlobals": [],
        "production": ["default"]
    }
}
